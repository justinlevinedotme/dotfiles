      "bold": false,
      "color": "#DA3036",
      "italic": false,
      "strikethrough": false,
      "tag": "- [ ] fix",
      "underline": false
    },
    {
      "backgroundColor": "transparent",
      "bold": false,
      "color": "#98C379",
      "italic": false,
      "strikethrough": false,
      "tag": "*",
      "underline": false
    }
  ],
  "claudeCode.useTerminal": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.detectIndentation": false,
  "editor.fontFamily": "Geist MonoNF, Fira Code, Menlo, Monaco, 'Courier New', monospace",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "editor.guides.indentation": true,
  "editor.minimap.enabled": false,
  "editor.overviewRulerBorder": false,
  "editor.renderWhitespace": "selection",
  "editor.showFoldingControls": "mouseover",
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "errorLens.decorations": {
    "errorMessage": {
      "color": "#DA3036",
      "infoColor": "#DA3036",
      "textDecoration": ";background:#551A1E;border-radius:0.3em;padding:0 0.5ch;"
    },
    "hintMessage": {
      "color": "#808080",
      "infoColor": "#808080",
      "textDecoration": ";background:rgba(128, 128, 128, 0.1); border-radius:0.3em;padding:0 0.5ch;"
    },
    "infoMessage": {
      "color": "#0C9784",
      "infoColor": "#0C9784",
      "textDecoration": ";background:#085E53; border-radius:0.3em;padding:0 0.5ch;"
    },
    "warningMessage": {
      "color": "#E79C13",
      "infoColor": "#E79C13",
      "textDecoration": ";background:#693F05;border-radius:0.3em;padding:0 0.5ch;"
    }
  },
  "errorLens.gutterIconsEnabled": true,
  "errorLens.messageBackgroundMode": "none",
  "errorLens.messageEnabled": true,
  "extensions.ignoreRecommendations": true,
  "files.exclude": {
    "**/.*": false,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/.tmp": true,
    "**/node_modules": true,
    "**/Thumbs.db": true,
    "**dist": true
  },
  "git.decorations.enabled": false,
  "git.ignoreRebaseWarning": true,
  "git.openRepositoryInParentFolders": "always",
  "indentRainbow.colors": ["#FFFFFF25", "#FFFFFF20", "#FFFFFF16", "#FFFFFF11"],
  "material-icon-theme.hidesExplorerArrows": true,
  "search.useGlobalIgnoreFiles": false,
  "search.useIgnoreFiles": true,
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "Geist Mono",
  "terminal.integrated.smoothScrolling": true,
  "todo-tree.filtering.excludeGlobs": [
    "**/node_modules/*",
    "**/.git/*",
    "**/README.md"
  ],
  "todo-tree.general.tagGroups": {
    "BUG": ["BUG", "FIXME", "bug", "fixme"],
    "MARK": ["MARK", "mark"],
    "TODO": ["TODO", "todo"]
  },
  "todo-tree.general.tags": [
    "TODO",
    "todo",
    "BUG",
    "bug",
    "FIXME",
    "fixme",
    "MARK",
    "mark"
  ],
  "todo-tree.highlights.customHighlight": {
    "BUG": {
      "icon": "bug",
      "iconColour": "#DA3036"
    },
    "MARK": {
      "icon": "bookmark",
      "iconColour": "#FFFFFF8A"
    },
    "TODO": {
      "icon": "checklist"
    }
  },
  "todo-tree.highlights.enabled": false,
  "todo-tree.tree.labelFormat": "${tag:uppercase} ${after} [${filename}]",
  "todo-tree.tree.scanAtStartup": false,
  "todo-tree.tree.tooltipFormat": "${filename}:[${line}] in ${filepath}",
  "workbench.colorTheme": "Vercel (Dark)",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.list.smoothScrolling": true,
  "workbench.settings.editor": "json",
  "files.associations": {
    "*.mdx": "markdown"
  },
  "githubLocalActions.dockerDesktopPath": "/Applications/Docker.app",
  "claudeCode.preferredLocation": "panel",
  "http.systemCertificatesNode": false,
  "window.newWindowProfile": "Default",
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*"
  },
  "explorer.confirmPasteNative": false,
  "github.copilot.nextEditSuggestions.enabled": true,
  "git.confirmSync": false
}
